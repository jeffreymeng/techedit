  {
	"hosting": {
	  "public": "/",
	  "ignore": [
		"firebase.json",
		"**/.*",
		"**/node_modules/**"
	  ],

	  "rewrites": [ {
		"source": "/f/**",
		"destination": "/edit.html"
	  },
		{
		  "source": "/embed/**",
		  "destination": "/embed.html"
		}],
	  "cleanUrls": true,
	  "trailingSlash": true


	}
  }
